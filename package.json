    "@storybook/react-vite": "^8.6.11",
    "@storybook/react-webpack5": "^8.6.11",
    "@storybook/test": "^8.6.11",
    "@types/node": "^22.13.14",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.49.0",
